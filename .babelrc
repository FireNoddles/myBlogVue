{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime",["prismjs", {
    "languages": ["sql", "go", "markup", "python"],
    "plugins": ["line-numbers"],
    "theme": "default",
    "css": true
  }]
]
}
